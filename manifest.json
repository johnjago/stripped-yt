{
  "description": "No related videos, no comments, no autoplay, no annotations.",
  "manifest_version": 2,
  "name": "Stripped YouTube",
  "version": "2.1.0",
  "homepage_url": "https://gitlab.com/johnjago/stripped-yt",
  "permissions": [
    "*://*.youtube.com/*"
  ],
  "icons": {
    "48": "icon/youtube-ico-48.png",
    "96": "icon/youtube-ico-96.png"
  },
  "applications": {
    "gecko": {
      "id": "stripped-yt@stripped-yt.com",
      "strict_min_version": "45.0"
    }
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "css": ["stripped-yt.css"],
      "js": ["stripped-yt.js"],
      "run_at": "document_start"
    }
  ]
}
